<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxy.sample.db.dao.SysUserDao">
    <!--通过账户查找用户-->
    <select id="findSysUserByAccount" parameterType="string" resultType="com.zxy.sample.db.entry.SysUser">
        SELECT account,name,password FROM sys_user WHERE account=#{account}
    </select>
</mapper>